CREATE TABLE TetrisGameEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    gameAsJson TEXT NOT NULL
);

insert:
INSERT OR REPLACE INTO TetrisGameEntity(id, gameAsJson)
VALUES(?,?);

getAll:
SELECT * FROM TetrisGameEntity
ORDER BY id DESC;

updateName:
UPDATE TetrisGameEntity
SET gameAsJson = :gameAsJson
WHERE id IS :id;

delete:
DELETE FROM TetrisGameEntity
WHERE id IS :id;
