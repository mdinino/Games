CREATE TABLE TicTacToeGameEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    gameAsJson TEXT NOT NULL
);

insert:
INSERT OR REPLACE INTO TicTacToeGameEntity(id, gameAsJson)
VALUES(?,?);

getAll:
SELECT * FROM TicTacToeGameEntity
ORDER BY id DESC;

updateName:
UPDATE TicTacToeGameEntity
SET gameAsJson = :gameAsJson
WHERE id IS :id;

delete:
DELETE FROM TicTacToeGameEntity
WHERE id IS :id;
